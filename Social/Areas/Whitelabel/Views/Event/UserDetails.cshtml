@model Social.Entity.Models.User
@{
    ViewData["Title"] = _localizer["UserDetails"];
    ViewData["PageTitle"] = _localizer["UserDetails"];
    Layout = "~/Areas/Admin/Views/Shared/_Layoutcontrolpanel.cshtml";
}
@section Styless{
    <style>
        .form-check.form-check-solid .form-check-input:checked {
            background-color: #00f78b !important;
        }
        /* padding-bottom and top for image */
        .mfp-no-margins img.mfp-img {
            padding: 0;
        }

        /* position of shadow behind the image */
        .mfp-no-margins .mfp-figure:after {
            top: 0;
            bottom: 0;
        }
        /* padding for main container */
        .mfp-no-margins .mfp-container {
            padding: 0;
        }


        /*

        for zoom animation
        uncomment this part if you haven't added this code anywhere else

        */


        .mfp-with-zoom .mfp-container,
        .mfp-with-zoom.mfp-bg {
            opacity: 0;
            -webkit-backface-visibility: hidden;
            -webkit-transition: all 0.3s ease-out;
            -moz-transition: all 0.3s ease-out;
            -o-transition: all 0.3s ease-out;
            transition: all 0.3s ease-out;
        }

        .mfp-with-zoom.mfp-ready .mfp-container {
            opacity: 1;
        }

        .mfp-with-zoom.mfp-ready.mfp-bg {
            opacity: 0.8;
        }

        .mfp-with-zoom.mfp-removing .mfp-container,
        .mfp-with-zoom.mfp-removing.mfp-bg {
            opacity: 0;
        }

        .show-read-more .more-text {
            display: none;
        }
    </style>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.13.1/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/magnific-popup.min.css" integrity="sha512-+EoPw+Fiwh6eSeRK7zwIKG2MA8i3rV/DGa3tdttQGgWyatG/SkncT53KHQaS5Jh9MNOT3dmFL0FjTY08And/Cw==" crossorigin="anonymous" referrerpolicy="no-referrer" />

}
@*<div id="kt_content_container" class="container-xxl">*@
<div id="" class="">
    <div class="d-flex flex-column flex-lg-row">
        <div class="flex-column flex-lg-row-auto w-lg-250px w-xl-350px mb-10">
            <div class="card mb-5 mb-xl-8">
                <div class="card-body">
                    <div class="d-flex flex-center flex-column py-5">

                        <div class="symbol symbol-100px symbol-circle mb-7">
                            <a class="image-magnific" href="@(Model.UserDetails.UserImage??"/assets/media/avatars/blank.png")" title="User Image">
                                <img class="mw-100 mh-100" src="@(Model.UserDetails.UserImage??"/assets/media/avatars/blank.png")" alt="Profile Image" />
                            </a>
                        </div>

                        <span class="fs-3 text-gray-800  fw-bolder mb-3">@Model.DisplayedUserName</span>


                        <div class="fw-bolder mb-3">
                            @_localizer["Bio"]
                            <i class="fas fa-exclamation-circle ms-2 fs-7" data-bs-toggle="popover" data-bs-trigger="hover" data-bs-html="true" data-bs-content="@(_localizer["More About User"])"></i>
                        </div>
                        <div class="d-flex flex-wrap flex-center">
                            @{ if (Model.UserDetails.listoftags.Count == 0)
                                {
                                    <div class="border border-gray-300 border-dashed rounded py-3 px-3 mb-3">
                                        <div class="fs-4 fw-bolder text-gray-700">
                                            <span class="w-75px">User has no Bio </span>
                                        </div>
                                    </div>
                                }
                                else
                                {
                                    <div class="border border-gray-300 border-dashed rounded py-3 px-3 mb-3">
                                        <div class="fs-4 fw-bolder text-gray-700">
                                            <span class="w-75px">@Model.UserDetails.bio </span>
                                        </div>
                                    </div>
                                }
                            }

                        </div>

                    </div>

                    <div class="d-flex flex-stack fs-4 py-3">
                        <div class="fw-bolder rotate collapsible" data-bs-toggle="collapse" href="#kt_user_view_details" role="button" aria-expanded="false" aria-controls="kt_user_view_details">
                            Details
                            <span class="ms-2 rotate-180">

                                <span class="svg-icon svg-icon-3">
                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                                        <path d="M11.4343 12.7344L7.25 8.55005C6.83579 8.13583 6.16421 8.13584 5.75 8.55005C5.33579 8.96426 5.33579 9.63583 5.75 10.05L11.2929 15.5929C11.6834 15.9835 12.3166 15.9835 12.7071 15.5929L18.25 10.05C18.6642 9.63584 18.6642 8.96426 18.25 8.55005C17.8358 8.13584 17.1642 8.13584 16.75 8.55005L12.5657 12.7344C12.2533 13.0468 11.7467 13.0468 11.4343 12.7344Z" fill="black" />
                                    </svg>
                                </span>
                            </span>
                        </div>
                    </div>
                    <div class="separator"></div>
                    <div id="kt_user_view_details" class="collapse show">
                        <div class="pb-5 fs-6">
                            <div class="fw-bolder mt-5">Gender</div>
                            <div class="text-gray-600">@(Model.UserDetails.Gender??"Not Selected")</div>
                            <div class="fw-bolder mt-5">Email</div>
                            <div class="text-gray-600">
                                @Model.Email
                            </div>
                            <div class="fw-bolder mt-5">Language</div>
                            <div class="text-gray-600">
                                @(Model.UserDetails.language?? "Not Selected")
                            </div>
                            <div class="fw-bolder mt-5">What User Looking For</div>
                            <div class="text-gray-600">
                                @(Model.UserDetails.whatAmILookingFor?? "Not Selected")
                            </div>
                            <div class="fw-bolder mt-5">Last Login</div>
                            <div class="text-gray-600">@(Model.LoggedinUser.OrderByDescending(x=>x.CreatedOn).FirstOrDefault()?.CreatedOn.ToString("dd M yyyy ,HH:mm") ??"user  Logged Out Now")</div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="card mb-5 mb-xl-8">
                <div class="card-header border-0">
                    <div class="card-title">
                        <h3 class="fw-bolder m-0">User Interestes</h3>
                    </div>
                </div>
                <div class="card-body pt-2">

                    @if (Model.UserDetails.listoftags.Count == 0)
                    {
                        <div class="fw-bold fs-6 text-gray-700">
                            User Not Select Any Interest
                        </div>
                    }
                    @foreach (var item in Model.UserDetails.listoftags)
                    {
                        <div style="width:fit-content" class="fw-bold fs-6 text-gray-700 notice mb-2 d-inline-block  bg-dark  rounded border-dark border">
                            <span class="text-gray-100"> @item.Interests.name</span>
                        </div>
                    }
                </div>
            </div>
            <div class="card mb-5 mb-xl-8">
                <div class="card-header border-0">
                    <div class="card-title">
                        <h3 class="fw-bolder m-0">User Best describs</h3>
                    </div>
                </div>
                <div class="card-body pt-2">
                    <div class="notice d-flex bg-light-primary rounded border-primary border border-dashed mb-9 p-6">
                        <div class="d-flex flex-stack flex-grow-1">
                            @if (Model.UserDetails.WhatBestDescripsMeList.Count == 0)
                            {
                                <div class="fw-bold fs-6 text-gray-700">
                                    User Not Select Any

                                </div>
                            }
                            @foreach (var item in Model.UserDetails.WhatBestDescripsMeList)
                            {
                                <div class="fw-bold fs-6 text-gray-700">
                                    @item.WhatBestDescripsMe.name
                                </div>
                            }

                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="flex-lg-row-fluid ms-lg-15">

            <ul class="nav nav-custom nav-tabs nav-line-tabs nav-line-tabs-2x border-0 fs-4 fw-bold mb-8">
                <li class="nav-item">
                    <a class="nav-link text-active-primary pb-4 active"
                       data-bs-toggle="tab" href="#UserEvents_tab">User Events</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-active-primary pb-4"
                       data-bs-toggle="tab" href="#ReportsCreatedByUser_tab">Reports Created By User</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link text-active-primary pb-4"
                       data-bs-toggle="tab" href="#OtherUserReports_tab">User Reported By Other</a>
                </li>
            </ul>

            <div class="tab-content" id="myTabContent">

                <div class="tab-pane fade show active" id="UserEvents_tab" role="tabpanel">
                    @{ var UserEvetntsData = Model.UserDetails.EventData.OrderByDescending(x => x.eventdate).ThenBy(x => x.Id).ToList();}
                    <div class="card card-flush mb-6 mb-xl-9">
                        <div class="card-header">
                            <div class="card-title flex-column">
                                @*<h2 class="mb-1">Reports Created By User</h2>*@
                                <div class="fs-6 fw-bold text-muted">@Model.DisplayedUserName Created @UserEvetntsData.Count Event</div>
                            </div>
                        </div>
                        <div class="card-body d-flex flex-column">
                            <table class="table align-middle table-row-dashed fs-6 gy-5 mb-0 mw-100" id="UserEventsTable">
                                <thead>
                                    <tr class="text-start text-muted fw-bolder fs-7 text-uppercase gs-0">
                                        <th class="min-w-50px">Event</th>
                                        <th class="min-w-125px">Event From</th>
                                        <th class="min-w-125px">Event To</th>
                                    </tr>
                                </thead>
                                <tbody class="fw-bold text-gray-600">
                                    @foreach (var item in UserEvetntsData)
                                    {
                                        <tr class="max-w-70px mw-100 cursor-pointer usereventrow" data-url="/admin/AdminEvent/EventDetails?EventID=@(item.EntityId)">
                                            <td class="d-flex align-items-center">
                                                <div class="symbol symbol-circle symbol-50px overflow-hidden me-3">
                                                    <div class="symbol-label">
                                                        <a class="image-magnific" href="@(item.image??"/assets/media/avatars/blank.png")" title="Event Image">
                                                            <img class="mw-100 mh-100" src="@(item.image??"/assets/media/avatars/blank.png")" alt="Event Image" />
                                                        </a>
                                                    </div>
                                                </div>
                                                <div class="d-flex flex-column">
                                                    <span class="text-dark text-hover-primary fw-bolder mb-1">
                                                        @item.Title
                                                        @if (item.UserId == Model.UserDetails.PrimaryId)
                                                        {
                                                            <span style="width:fit-content" class="border text-primary pl-3 border-dashed border-primary">Admin</span>
                                                        }
                                                    </span>
                                                   
                                                    <span class="fw-bolder show-read-more">@item.description</span>
                                                </div>
                                            </td>
                                            <td>@(item.eventdate?.ToString("dd MMM yyyy")??""+item.eventfrom?.ToString(",hh:mm tt"))</td>
                                            <td>@(item.eventdateto?.ToString("dd MMM yyyy")??""+item.eventto?.ToString(",hh:mm tt"))</td>

                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>

                    </div>
                </div>
                <div class="tab-pane fade" id="ReportsCreatedByUser_tab" role="tabpanel">
                    <div class="card card-flush mb-6 mb-xl-9">
                        <div class="card-header">
                            <div class="card-title flex-column">
                                @*<h2 class="mb-1">Reports Created By User</h2>*@
                                <div class="fs-6 fw-bold text-muted">@Model.UserReports_CreatedBy.Count Report Created By  @Model.DisplayedUserName</div>
                            </div>
                        </div>
                        <div class="card-body d-flex flex-column">
                            <table class="table align-middle table-row-dashed fs-6 gy-5 mb-0 mw-100" id="ReportsCreatedByUserTable">
                                <thead>
                                    <tr class="text-start text-muted fw-bolder fs-7 text-uppercase gs-0">
                                        <th class="min-w-50px">Reported</th>
                                        <th class="min-w-125px">Report reason</th>
                                        <th class="min-w-125px"> message</th>
                                        <th class="min-w-125px">Report Date</th>
                                    </tr>
                                </thead>
                                <tbody class="fw-bold text-gray-600">
                                    @foreach (var item in Model.UserReports_CreatedBy.OrderByDescending(x => x.RegistrationDate))
                                    {
                                        <tr>
                                            <td class="d-flex align-items-center">
                                                <div class="symbol symbol-circle symbol-50px overflow-hidden me-3">
                                                    <div class="symbol-label">
                                                        <a class="image-magnific" href="@(item.ReportedUser.UserDetails.UserImage??"/assets/media/avatars/blank.png")" title="User Image">
                                                            <img class="mw-100 mh-100" src="@(item.ReportedUser.UserDetails.UserImage??"/assets/media/avatars/blank.png")" alt="Profile Image" />
                                                        </a>
                                                    </div>
                                                </div>
                                                <div class="d-flex flex-column">
                                                    <span class="text-dark text-hover-primary fw-bolder mb-1">@item.ReportedUser.DisplayedUserName</span>
                                                    <span class="fw-bolder">@item.ReportedUser.Email</span>
                                                </div>
                                            </td>
                                            <td>@item.ReportReason.Name</td>
                                            <td>@item.Message</td>
                                            <td>@(item.RegistrationDate?.ToString("dd MMM yyyy ,hh:mm tt")??"")</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>

                    </div>
                </div>
                <div class="tab-pane fade" id="OtherUserReports_tab" role="tabpanel">

                    <div class="card card-flush mb-6 mb-xl-9">
                        <div class="card-header">

                            <div class="card-title flex-column">
                                @*<h2 class="mb-1">User Reported By Other</h2>*@
                                <div class="fs-6 fw-bold text-muted">Other User Report @Model.DisplayedUserName By @Model.UserReports_Reported.Count Report  </div>
                            </div>
                            <div class="card-toolbar">

                            </div>
                        </div>

                        <div class="card-body d-flex flex-column">
                            <table class="table align-middle table-row-dashed fs-6 gy-5 mb-0 mw-100" id="OtherUserReportsTable">
                                <thead>
                                    <tr class="text-start text-muted fw-bolder fs-7 text-uppercase gs-0">
                                        <th class="min-w-50px">Reporter</th>
                                        <th class="min-w-125px">Report reason</th>
                                        <th class="min-w-125px"> message</th>
                                        <th class="min-w-125px">Report Date</th>
                                    </tr>
                                </thead>
                                <tbody class="fw-bold text-gray-600">
                                    @foreach (var item in Model.UserReports_Reported.OrderByDescending(x => x.RegistrationDate))
                                    {
                                        <tr>
                                            <td class="d-flex align-items-center">
                                                <div class="symbol symbol-circle symbol-50px overflow-hidden me-3">
                                                    <div class="symbol-label">
                                                        <a class="image-magnific" href="@(item.CreatedBy_User.UserDetails.UserImage??"/assets/media/avatars/blank.png")" title="User Image">
                                                            <img class="mw-100 mh-100" src="@(item.CreatedBy_User.UserDetails.UserImage??"/assets/media/avatars/blank.png")" alt="Profile Image" />
                                                        </a>
                                                    </div>
                                                </div>
                                                <div class="d-flex flex-column">
                                                    <span class="text-dark text-hover-primary fw-bolder mb-1">@item.CreatedBy_User.DisplayedUserName</span>
                                                    <span class="fw-bolder">@item.CreatedBy_User.Email</span>
                                                </div>
                                            </td>
                                            <td>@item.ReportReason.Name</td>
                                            <td>@item.Message</td>
                                            <td>@(item.RegistrationDate?.ToString("dd MMM yyyy ,hh:mm tt")??"")</td>

                                        </tr>
                                    }
                                </tbody>

                            </table>
                        </div>

                    </div>
                </div>
                <div class="tab-pane fade" id="kt_user_view_overview_security" role="tabpanel">
