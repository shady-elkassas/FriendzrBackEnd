                    height: 32px !important;
                }

                .flatpickr-prev-month {
                    left: unset !important;
                    right: 0 !important;
                    transform: scaleX(-1) !important;
                }

                .flatpickr-next-month {
                    right: unset !important;
                    left: 0 !important;
                    transform: scaleX(-1) !important;
                }

