    <script>
        function AssignUserLanguage() {
            if (CurrentUserLanguage.toLocaleLowerCase() == "ar-eg") {
                $("#SpanLanguage").text("العربية");
                $("#ImageLanguage").attr("src", "/Assets/media/flags/egypt.svg");
            }
            else {
                $("#SpanLanguage").text("English");
                $("#ImageLanguage").attr("src", "/Assets/media/flags/united-states.svg");
            }
        }
        function ChangeUserLanguage(Language) {
            let DataToSent = new FormData();
            DataToSent.append("culture", Language);
            $.ajax({
                "url": "/api/Account/SetLanguage",
                "method": "Post",
                "ContentType": "application/x-www-form-urlencoded",

                data: { "culture": Language },
                success: function (res) {

                    location.reload();
                },
                error: function () {
                    //pushNotification("danger", getToken("someThingWentWrong"));
                }
            });

        }
        function LogOut() {
            $.post("/Whitelabel/Account/LogOut", (res) => {
                location.reload();
            })
        }
        $(document).ready(function () {

            $("[id='#kt_aside_menu']").find("a[href='@requestUserUrl']").addClass("active");

            AssignUserLanguage();

        })
    </script>
